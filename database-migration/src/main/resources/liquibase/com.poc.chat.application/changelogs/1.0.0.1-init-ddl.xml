<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">

    <changeSet dbms="postgresql" author="chat-application-db" id="chat-application-db-rollback-tag-1.0.0.1">
        <tagDatabase tag="chat-application-db-rollback-tag-1.0.0.1"/>
    </changeSet>

    <changeSet dbms="postgresql" author="chat-application-db" id="1.0.0.1-1">
        <createTable tableName="rest_audit">
            <column autoIncrement="true" name="id" type="BIGINT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="rest_audit_pk"/>
            </column>
            <column name="endpoint" type="TEXT"/>
            <column name="input" type="TEXT"/>
            <column name="created_at" type="TIMESTAMP"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
